#include <iostream>
#include <string>

using namespace std; 

int main() {
    string nome, nomecaro;
    float prc,quant,total,max;
    total=0;max=-9999;
    while (nome!="fim"){
        cout<<"Introduza a quantidade comprada:";
        cin>>quant;
        total=total+prc*quant;
    }

    return 0;
}
